algoritmo "contarAte10"
// Algoritmo que conta de 1 até 10 utilizando a estrutura de repetição REPITA.
var
   contador: Inteiro
inicio
      Escreval("-----------")
      Escreval(" CONTADOR: ")
      Escreval("-----------")
      Escreval("")
      
      contador <- 1
      Repita
         Escreval("Contador: ", contador)
         contador <- contador + 1
      Ate (contador > 10)
fimalgoritmo
